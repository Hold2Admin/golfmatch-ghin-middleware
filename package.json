  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-secrets": "^4.7.0",
    "applicationinsights": "^2.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mssql": "^12.2.0",
    "redis": "^4.6.11",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "jest": {
